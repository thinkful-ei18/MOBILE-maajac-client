{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native",
    "import",
    "jsx-a11y"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "react-native/react-native": true,
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}